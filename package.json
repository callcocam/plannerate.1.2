{
  "name": "plannerate-vue",
  "version": "1.0.0",
  "description": "Frontend Vue para o pacote Laravel Plannerate",
  "type": "module",
  "main": "dist/plannerate.umd.js",
  "module": "dist/plannerate.es.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/plannerate.es.js",
      "require": "./dist/plannerate.umd.js"
    },
    "./style.css": "./dist/css/app.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --emitDeclarationOnly && vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vue-tsc": "^1.8.1"
  },
  "peerDependencies": {
    "pinia": "^2.0.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "engines": {
    "node": ">=16"
  }
}
